alias pbcopy="xclip -i -selection clipboard"
alias pbpaste="xclip -o -selection clipboard"
alias pbc=pbcopy
alias pbp=pbpaste

alias emacs="emacs -nw"

alias g="git status"
alias gl="git pull"
alias go="git log"
alias gp="git push"
alias gd="git diff"
alias gcm="git commit -m"
alias gad="git add -A"
