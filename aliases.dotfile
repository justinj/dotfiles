alias -g pbc="xclip -i -selection clipboard"
alias -g pbp="xclip -o -selection clipboard"

alias la="ls -a"

alias ijs="node"
alias clj="lein repl"

function rinit() {
  git init
  rspec --init
  mkdir lib
  git add -A
  git commit -m "Initial commit"
}

alias g="git status"
alias gl="git pull"
alias gp="git push"
alias gd="git diff"
